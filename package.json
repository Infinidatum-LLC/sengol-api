{
  "name": "sengol-api",
  "version": "1.0.0",
  "description": "Sengol Middleware API - Business logic layer",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/app.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "api",
    "middleware",
    "sengol"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.10.0",
    "@qdrant/js-client-rest": "1.12.0",
    "@upstash/redis": "^1.35.6",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.0",
    "lru-cache": "^11.2.2",
    "openai": "^6.8.1",
    "stripe": "^20.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.19",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
