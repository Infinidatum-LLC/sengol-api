    "@prisma/client": "^5.10.0",
    "d-vecdb": "^0.2.2",
    "openai": "^4.28.0",
    "dotenv": "^16.4.5",
    "zod": "^3.22.4",
    "bcrypt": "^5.1.1",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/bcrypt": "^5.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
